{
  "section_title": "Hablemos de tu proyecto",
  "section_subtitle": "Estoy aquí para ayudarte a capturar esos momentos especiales. Contáctame y conversemos sobre cómo puedo hacer realidad tu visión fotográfica.",
  "form": {
    "action": "/contact",
    "method": "POST",
    "fields": {
      "name": {
        "label": "Nombre Completo",
        "required": true,
        "placeholder": "Tu nombre completo"
      },
      "email": {
        "label": "Email",
        "required": true,
        "placeholder": "tu@email.com"
      },
      "phone": {
        "label": "Teléfono",
        "required": false,
        "placeholder": "+34 600 123 456"
      },
      "service": {
        "label": "Tipo de Sesión",
        "required": true,
        "placeholder": "Selecciona un servicio",
        "options": [
          { "value": "retratos", "label": "Retratos Profesionales" },
          { "value": "bodas", "label": "Bodas & Eventos" },
          { "value": "familias", "label": "Sesiones Familiares" },
          { "value": "otro", "label": "Otro (especificar en mensaje)" }
        ]
      },
      "date": {
        "label": "Fecha Preferida",
        "required": false
      },
      "message": {
        "label": "Mensaje",
        "required": true,
        "placeholder": "Cuéntame más sobre tu visión para la sesión..."
